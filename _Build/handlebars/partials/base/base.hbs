<!doctype html>
<html lang="en">

    {{> head}}

    <body>
        {{> svgSprite}}

        <main class="container">
        	{{> (lookup . 'baseInject')}}
    	</main>

        {{> footer}}

        <script type="text/javascript">
            function baseJS(){
                chartManager.charts = [
                    {{#each index.charts}}
                        {
                            "name": "{{name}}"
                        },
                    {{/each}}
                ];
            }
        </script>

        <script type="text/javascript" src="js/script.js"></script>
    </body>
</html>